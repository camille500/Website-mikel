<script type="text/javascript">
  var iOS = parseFloat(
    ('' + (/CPU.*OS ([0-9_]{1,5})|(CPU like).*AppleWebKit.*Mobile/i.exec(navigator.userAgent) || [0, ''])[1])
    .replace('undefined', '3_2').replace('_', '.').replace('_', '')
  ) || false;

  var iPhone4 = navigator.userAgent.match(/iPhone/i) !== null  && window.screen.height == (960 / 2);
  if (document.documentMode || /Edge/.test(navigator.userAgent) || iPhone4 || iOS < 10) {
    document.write('<script src="/js/scriptie.js"><\/script>');
  } else {
    document.write('<script src="/js/script.js"><\/script>');
  }
</script>

<script>
  console.log(iOS)
</script>
</body>

