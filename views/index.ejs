  <% let count = 0; %>
  <% images.forEach(function(image) { %>
    <% if(count === 0)  { %>
      <section class="__image_wrapper">
      <img src="/images/<%= image %>" class="__normal" />
    <% } %>
    <% count++ %>
    <% if(count === 2)  { %>
      <img src="/images/<%= image %>" class="__negative" />
      </section>
      <% count = 0; %>
    <% } %>
  <% }) %>

